{
  "name": "wrapper-validation-action",
  "version": "0.0.0",
  "description": "Validates Gradle Wrapper JAR Files",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gradle/wrapper-validation-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Gradle",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.2.6",
    "typed-rest-client": "1.7.3",
    "unhomoglyph": "1.0.6"
  },
  "devDependencies": {
    "@types/jest": "26.0.14",
    "@types/node": "12.12.62",
    "@typescript-eslint/parser": "4.2.0",
    "@vercel/ncc": "^0.28.5",
    "eslint": "7.9.0",
    "eslint-plugin-github": "4.1.1",
    "eslint-plugin-jest": "24.0.2",
    "jest": "26.4.2",
    "jest-circus": "26.4.2",
    "js-yaml": "3.14.0",
    "prettier": "2.1.2",
    "ts-jest": "26.4.0",
    "typescript": "4.0.3"
  }
}
